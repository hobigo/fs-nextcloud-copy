    "@types/dotenv": "^8.2.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.12",
    "chai": "^4.2.0",
    "prettier": "^1.18.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "bugs": {
    "url": "https://github.com/hobigo/fs-nextcloud-copy/issues"

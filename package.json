    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start": "node ./dist/main.js"
  },
  "dependencies": {
    "nextcloud-node-client": "^1.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",

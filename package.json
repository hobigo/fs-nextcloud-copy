  "keywords": [
    "nextcloud",
    "file system",
    "copy",
    "upload",
    "file"
  ],
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hobigo/fs-nextcloud-copy.git"
  },
  "scripts": {
